<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.projectone.measureExp.expController">
   <children>
      <Label alignment="CENTER" prefHeight="37.0" prefWidth="600.0" style="-fx-background-color: #1269b1;" text="Measurement Explorer" textFill="WHITE">
         <font>
            <Font name="Comic Sans MS Bold" size="18.0" />
         </font>
      </Label>
      <ComboBox fx:id="getOrderStatus" layoutX="37.0" layoutY="68.0" onAction="#getViaOrderStatus" prefWidth="150.0" />
      <ComboBox fx:id="getWorker" layoutX="225.0" layoutY="68.0" onAction="#getViaWorker" prefWidth="150.0" />
      <TextField fx:id="getMob" layoutX="416.0" layoutY="68.0" />
      <Button centerShape="false" layoutX="134.0" layoutY="356.0" mnemonicParsing="false" onAction="#doShowAll" prefHeight="25.0" prefWidth="168.0" text="Show All">
         <font>
            <Font name="Times New Roman" size="13.0" />
         </font>
         <effect>
            <DropShadow blurType="ONE_PASS_BOX" height="8.53" offsetX="-1.0" offsetY="1.0" radius="3.6325000000000003" spread="0.5" width="8.0" />
         </effect>
      </Button>
      <Button centerShape="false" layoutX="329.0" layoutY="357.0" mnemonicParsing="false" onAction="#doExport" prefHeight="25.0" prefWidth="168.0" text="Export all records to Excel">
         <font>
            <Font name="Times New Roman" size="13.0" />
         </font>
         <effect>
            <DropShadow blurType="ONE_PASS_BOX" height="8.53" offsetX="-1.0" offsetY="1.0" radius="3.6325000000000003" spread="0.5" width="8.0" />
         </effect>
      </Button>
      <Button centerShape="false" layoutX="473.0" layoutY="100.0" mnemonicParsing="false" onAction="#doFetch" prefHeight="25.0" prefWidth="91.0" text="Fetch">
         <font>
            <Font name="Times New Roman" size="13.0" />
         </font>
         <effect>
            <DropShadow blurType="ONE_PASS_BOX" height="8.53" offsetX="-1.0" offsetY="1.0" radius="3.6325000000000003" spread="0.5" width="8.0" />
         </effect>
      </Button>
      <Label layoutX="37.0" layoutY="49.0" prefHeight="17.0" prefWidth="80.0" text="Order Status">
         <font>
            <Font name="Times New Roman Bold" size="14.0" />
         </font>
      </Label>
      <Label layoutX="225.0" layoutY="50.0" prefHeight="17.0" prefWidth="80.0" text="Worker">
         <font>
            <Font name="Times New Roman Bold" size="14.0" />
         </font>
      </Label>
      <Label layoutX="418.0" layoutY="50.0" prefHeight="17.0" prefWidth="159.0" text="Get records by Mobile No.">
         <font>
            <Font name="Times New Roman Bold" size="14.0" />
         </font>
      </Label>
      <TableView fx:id="getRecords" layoutX="23.0" layoutY="142.0" prefHeight="200.0" prefWidth="553.0" />
   </children>
</AnchorPane>
