      <Label layoutX="318.0" layoutY="349.0" prefHeight="18.0" prefWidth="75.0" text="Total Bill:-">
         <font>
            <Font name="Times New Roman" size="15.0" />
         </font>
      </Label>
      <TextField fx:id="getTotal" layoutX="401.0" layoutY="346.0" />
      <Button layoutX="211.0" layoutY="388.0" mnemonicParsing="false" onAction="#doAll" prefHeight="25.0" prefWidth="185.0" text="Delivered All">
         <font>
            <Font name="Arial" size="13.0" />
         </font>
         <effect>
            <DropShadow blurType="ONE_PASS_BOX" height="3.0" offsetX="-2.0" offsetY="2.0" radius="1.0" width="3.0" />
         </effect>
      </Button>
      <Label layoutX="68.0" layoutY="48.0" text="Enter Mobile No.">
         <font>
            <Font name="Times New Roman" size="16.0" />
         </font>
      </Label>
      <Label layoutX="68.0" layoutY="110.0" text="Status">
         <font>
            <Font name="Times New Roman" size="16.0" />
         </font>
      </Label>
      <Label layoutX="132.0" layoutY="110.0" text="Order Id">
         <font>
            <Font name="Times New Roman" size="16.0" />
         </font>
      </Label>
      <Label layoutX="280.0" layoutY="110.0" text="Bill">
         <font>
            <Font name="Times New Roman" size="16.0" />
         </font>
      </Label>
      <Label layoutX="428.0" layoutY="110.0" text="Item/Dress">
         <font>
            <Font name="Times New Roman" size="16.0" />
         </font>
      </Label>
